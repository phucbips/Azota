{
  "version": 2,
  "builds": [
    {
      "src": "src/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "../build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "functions": {
    "src/pages/api/*.js": {
      "maxDuration": 30
    }
  }
}
